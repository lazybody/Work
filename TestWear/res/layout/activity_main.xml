<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="${relativePackage}.${activityClass}" >

    <Button 
        android:id="@+id/send_notify"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/send_notifi"/>
    <Button 
        android:id="@+id/send_notify1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/send_notify"
        android:text="@string/send_notifi"/>
    
    <Button
        android:id="@+id/notifi_listener"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/send_notify1"
        android:text="@string/send_notifi"
        />
    
    <Button
        android:id="@+id/notifi_intent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/notifi_listener"
        android:text="@string/send_notifi"
        />

</RelativeLayout>
